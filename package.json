{
  "name": "pdb-attach",
  "description": "A python debugger that can attach to running processes.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smitchell556/pdb-attach.git"
  },
  "keywords": [],
  "author": "",
  "private": true,
  "bugs": {
    "url": "https://github.com/smitchell556/pdb-attach/issues"
  },
  "homepage": "https://github.com/smitchell556/pdb-attach#readme",
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.3.0"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "export PBR_VERSION=${nextRelease.version} && python setup.py sdist bdist_wheel",
          "publishCmd": "twine upload dist/*"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "MANIFEST.in",
            "AUTHOR"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
